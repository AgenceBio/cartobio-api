<?xml version="1.0" encoding="UTF-8" ?>
<xsd:schema targetNamespace="urn:x-telepac:fr.gouv.agriculture.telepac:echange-producteur" xmlns="urn:x-telepac:fr.gouv.agriculture.telepac:echange-producteur"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:gml="http://www.opengis.net/gml" elementFormDefault="qualified" attributeFormDefault="unqualified"
            version="2024-V4">
    <!-- Import du schema GML (Geometry Markeup Language) (cf. http://www.opengis.net/gml/) -->
    <xsd:import namespace="http://www.opengis.net/gml" schemaLocation="http://schemas.opengis.net/gml/2.1.2/geometry.xsd"/>

    <!-- element Racine -->
    <xsd:element name="producteurs">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="producteur" type="Producteur" maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <!-- type Producteur -->
    <xsd:complexType name="Producteur">
        <xsd:all>
            <xsd:element name="demandeur" type="Demandeur" minOccurs="0"/>
            <xsd:element name="rpg" type="RPG" minOccurs="0"/>
            <xsd:element name="maec-api" type="EngagementAPI" minOccurs="0"/>
            <xsd:element name="maec-prm" type="EngagementsPRM" minOccurs="0"/>
            <xsd:element name="maec-prv" type="EngagementPRV" minOccurs="0"/>
            <xsd:element name="effectifs-animaux" type="EffectifsAnimaux" minOccurs="0"/>
            <xsd:element name="demandes-aides-pilier1-et-AR" type="DemandesAidesPilier1EtAR" minOccurs="0"/>
            <xsd:element name="demandes-aides-posei-dom" type="DemandesAidesPoseiDom" minOccurs="0"/>
            <xsd:element name="demandes-aides-pilier1-mayotte" type="DemandesAidesPilier1Mayotte" minOccurs="0"/>
            <xsd:element name="demandes-aides-pilier2" type="DemandesAidesPilier2" minOccurs="0"/>
            <xsd:element name="pieces-jointes" type="PiecesJointes" minOccurs="0"/>
            <xsd:element name="bloc-note" type="BlocNote" minOccurs="0"/>
            <xsd:element name="autres-obligations" type="AutresObligations" minOccurs="0" maxOccurs="1"/>
        </xsd:all>
        <xsd:attribute name="campagne" type="Campagne"/>
        <xsd:attribute name="numero-pacage" type="NumeroPacage" use="required"/>
        <xsd:attribute name="fichier-xsd" type="NomFichierXSD" use="required"/>
    </xsd:complexType>

    <!-- type Demandeur -->
    <xsd:complexType name="Demandeur">
        <xsd:sequence>
            <xsd:choice minOccurs="1" maxOccurs="1">
                <xsd:element name="identification-individuelle" type="IdentificationIndividuelle"/>
                <xsd:element name="identification-societe" type="IdentificationSociete"/>
            </xsd:choice>
            <xsd:choice minOccurs="0" maxOccurs="1">
                <xsd:element name="siret" type="NumeroSiret" minOccurs="0" maxOccurs="1"/>
                <xsd:element name="demarches-siret-encours" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
                <xsd:element name="cas-derogatoire" type="CasDerogatoire" minOccurs="0" maxOccurs="1"/>
            </xsd:choice>
            <xsd:element name="iban" type="Iban" minOccurs="0"/>
            <xsd:element name="courriel" type="Email" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
        <xsd:attribute name="certificat-environnemental" type="xsd:boolean" use="optional"/>
        <xsd:attribute name="dossier-sans-demande-aides" type="xsd:boolean" use="optional"/>
    </xsd:complexType>

    <!-- type IdentificationIndividuelle -->
    <xsd:complexType name="IdentificationIndividuelle">
        <xsd:all>
            <xsd:element name="identite" type="Identite" minOccurs="1"/>
        </xsd:all>
        <xsd:attribute name="revenus-retraite" type="MontantRevenus" use="optional"/>
        <xsd:attribute name="numero-fiscal" type="NumeroFiscal" use="optional"/>
    </xsd:complexType>

    <!-- type IdentificationSociete -->
    <xsd:complexType name="IdentificationSociete">
        <xsd:all>
            <xsd:element name="exploitation" type="DenominationExploitation" minOccurs="1"/>
            <xsd:element name="associes" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="associe" type="Associe" minOccurs="1" maxOccurs="10"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>
    <!-- type Associe -->
    <xsd:complexType name="Associe">
        <xsd:all>
            <xsd:element name="identite" type="Identite" minOccurs="1"/>
            <xsd:element name="revenus-retraite" type="MontantRevenus" minOccurs="0"/>
            <xsd:element name="numero-fiscal" type="NumeroFiscal" minOccurs="0"/>
        </xsd:all>
        <xsd:attribute name="numero-pacage" type="NumeroPacage" use="required"/>
    </xsd:complexType>

    <!-- type CasDerogatoire -->
    <xsd:complexType name="CasDerogatoire">
        <xsd:all>
            <xsd:element name="cas-derogatoire-val" type="xsd:boolean" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="justif-absence-siret" type="JustificationSIRET" minOccurs="1" maxOccurs="1"/>
        </xsd:all>
    </xsd:complexType>

    <!-- type EngagementAPI -->
    <xsd:complexType name="EngagementAPI">
        <xsd:sequence>
            <xsd:element name="engagement-api" type="EngagementApiParCampagneEngagement" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute name="nombre-emplacements" type="NombreEmplacements" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="EngagementApiParCampagneEngagement">
        <xsd:sequence>
            <xsd:element name="campagneEngagement" type="CampagneEngagementType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="nombre-colonies" type="NombreColonies" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>


    <!-- type EngagementPRV -->
    <xsd:complexType name="EngagementPRV">
        <xsd:sequence>
            <xsd:element name="engagement-prv" type="EngagementPrvParCampagneEngagement" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="EngagementPrvParCampagneEngagement">
        <xsd:sequence>
            <xsd:element name="campagneEngagement" type="CampagneEngagementType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="surface-cultures-perennes" type="SurfaceAres" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="surface-cultures-annuelles" type="SurfaceAres" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- type EngagementsPRM -->
    <xsd:complexType name="EngagementsPRM">
        <xsd:sequence>
            <xsd:element name="femelle-reproductrice" type="NombreAnimauxParEspecePRM" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="male-reproducteur" type="NombreAnimauxParEspecePRM" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="identification-equin-asin" type="IdentificationAnimalPRM" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="organisme-gestionnaire-race" type="OrganismeGestionnaireRacePRM" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- type NombreAnimauxParEspecePRM -->
    <xsd:complexType name="NombreAnimauxParEspecePRM">
        <xsd:sequence>
            <xsd:element name="nombre-animaux" type="NombreAnimauxParRacePRM" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute name="espece" type="EspecePRM" use="required"/>
        <xsd:attribute name="campagneEngagement" type="CampagneEngagementType" use="optional"/>
    </xsd:complexType>

    <!-- type IdentificationAnimalPRM -->
    <xsd:complexType name="IdentificationAnimalPRM">
        <xsd:attribute name="race" type="RacePRM" use="required"/>
        <xsd:attribute name="sexe" type="SexeType" use="required"/>
        <xsd:attribute name="identite-animal" type="IdentiteAnimalPRM" use="required"/>
    </xsd:complexType>

    <!-- type OrganismeGestionnaireRacePRM -->
    <xsd:complexType name="OrganismeGestionnaireRacePRM">
        <xsd:attribute name="race" type="RacePRM" use="required"/>
        <xsd:attribute name="organisme-race" type="OrganismeRacePRM" use="required"/>
    </xsd:complexType>

    <!-- type NombreAnimauxParRacePRM -->
    <xsd:complexType name="NombreAnimauxParRacePRM">
        <xsd:attribute name="race" type="RacePRM" use="required"/>
        <xsd:attribute name="nombre-animaux" type="NbAnimauxType1" use="required"/>
    </xsd:complexType>

    <!-- type Demandes Aides pilier 1 -->
    <xsd:complexType name="DemandesAidesPilier1EtAR">
        <xsd:sequence>
            <xsd:element name="bcae8" type="Bcae8" minOccurs="0"/>
            <xsd:element name="demande-aides-decouplees" type="AidesDecouplees" minOccurs="1"/>
            <xsd:element name="demande-aide-ecoregime" type="AideEcoregime" minOccurs="0"/>
            <xsd:element name="demande-legumineuses-fourrageres" type="LegumineusesFourrageres" minOccurs="1"/>
            <xsd:element name="demande-legumineuses-graines" type="LegumineusesGraines" minOccurs="0"/>
            <xsd:element name="demande-assurance-recolte" type="AssuranceRecolte" minOccurs="1"/>
        </xsd:sequence>
        <xsd:attribute name="aide-jeunes-agriculteurs" type="xsd:boolean" use="required"/>
        <xsd:attribute name="ble-dur" type="xsd:boolean" use="required"/>
        <xsd:attribute name="prunes-transformation" type="xsd:boolean" use="required"/>
        <xsd:attribute name="cerises-transformation" type="xsd:boolean" use="required"/>
        <xsd:attribute name="peches-transformation" type="xsd:boolean" use="required"/>
        <xsd:attribute name="poires-transformation" type="xsd:boolean" use="required"/>
        <xsd:attribute name="tomates-industrie" type="xsd:boolean" use="required"/>
        <xsd:attribute name="pommes-terre-feculieres" type="xsd:boolean" use="required"/>
        <xsd:attribute name="chanvre" type="xsd:boolean" use="required"/>
        <xsd:attribute name="houblon" type="xsd:boolean" use="required"/>
        <xsd:attribute name="semences-graminees" type="xsd:boolean" use="required"/>
        <xsd:attribute name="riz" type="xsd:boolean" use="required"/>
        <xsd:attribute name="maraichage" type="xsd:boolean" use="optional"/>
    </xsd:complexType>


    <xsd:complexType name="Bcae8">
        <xsd:sequence>
            <xsd:element name="option-BCAE8" type="optionBcae8"/>
        </xsd:sequence>
   </xsd:complexType>

    <xsd:complexType name="DemandesAidesPoseiDom">
        <xsd:attribute name="tonne-de-canne-livree" type="xsd:boolean" use="required"/>
        <xsd:attribute name="aide-surcouts-production-canne" type="xsd:boolean" use="optional"/>
			<!-- la valeur "aide-surcouts-production-canne" doit rester optionelle pour la campagne n-1 (2023)-->
    </xsd:complexType>
    <xsd:complexType name="DemandesAidesPilier1Mayotte">
        <xsd:sequence>
            <xsd:element name="demande-structure-collective" type="StructureCollective" minOccurs="1"/>
        </xsd:sequence>
        <xsd:attribute name="aide-de-base" type="xsd:boolean" use="required"/>
        <xsd:attribute name="vanille" type="xsd:boolean" use="required"/>
        <xsd:attribute name="ylang-ylang" type="xsd:boolean" use="required"/>
        <xsd:attribute name="nouvel-installe" type="xsd:boolean" use="required"/>
    </xsd:complexType>

    <!-- type AideEcoregime -->
    <xsd:complexType name="AideEcoregime">
        <xsd:attribute name="aide-ecoregime" type="xsd:boolean" use="required"/>
        <xsd:attribute name="voie-ecoregime" type="VoieEcoregime" use="optional"/>
        <xsd:attribute name="bonus-haie" type="xsd:boolean" use="optional"/>
        <xsd:attribute name="certification" type="Certification" use="optional"/>
    </xsd:complexType>


    <!-- type AidesDecouplees -->
    <xsd:complexType name="AidesDecouplees">
        <xsd:attribute name="aides-decouplees" type="xsd:boolean" use="required"/>
    </xsd:complexType>

    <!-- type LegumineusesFourrageres -->
    <xsd:complexType name="LegumineusesFourrageres">
        <xsd:attribute name="legumineuse-fourragere" type="xsd:boolean" use="required"/>
        <xsd:attribute name="demandeur-eleveur" type="xsd:boolean" use="optional"/>
        <xsd:attribute name="numero-pacage-eleveur" type="NumeroPacage" use="optional"/>
    </xsd:complexType>

    <!-- type LegumineusesGraines -->
    <xsd:complexType name="LegumineusesGraines">
        <xsd:attribute name="legumineuse-graine" type="xsd:boolean" use="required"/>
        <xsd:attribute name="contrat-deshydratation" type="xsd:boolean" use="optional"/>
    </xsd:complexType>

    <!-- type AssuranceRecolte -->
    <xsd:complexType name="AssuranceRecolte">
        <xsd:sequence>
            <xsd:element name="assureurs-avec-accord-diffusion" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="assureur" type="CodeAssureur" minOccurs="1" maxOccurs="3"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="assurance-recolte" type="xsd:boolean" use="required"/>
    </xsd:complexType>

    <!-- type StructureCollective -->
    <xsd:complexType name="StructureCollective">
        <xsd:attribute name="structure-collective" type="xsd:boolean" use="required"/>
        <xsd:attribute name="secteur-vegetale" type="xsd:boolean" use="optional"/>
        <xsd:attribute name="secteur-animale" type="xsd:boolean" use="optional"/>
    </xsd:complexType>

    <!-- type Demandes Aides pilier 2 -->
    <xsd:complexType name="DemandesAidesPilier2">
        <xsd:sequence>
            <xsd:element name="ichn" type="ICHN" minOccurs="1"/>
        </xsd:sequence>
        <xsd:attribute name="demande-ab" type="xsd:boolean" use="required"/>
        <xsd:attribute name="demande-ab-rdr4" type="xsd:boolean" use="optional"/>
        <xsd:attribute name="demande-maec" type="xsd:boolean" use="required"/>
        <xsd:attribute name="demande-maec-rdr4" type="xsd:boolean" use="optional"/>
    </xsd:complexType>
    <!-- type ICHN -->
    <xsd:complexType name="ICHN">
        <xsd:attribute name="demande-ichn" type="xsd:boolean" use="required"/>
        <xsd:attribute name="pension-reversion" type="xsd:boolean" use="optional"/>
    </xsd:complexType>

    <!-- type Effectifs Animaux -->
    <xsd:complexType name="EffectifsAnimaux">
        <xsd:sequence>
            <xsd:element name="effectif-animal" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:choice minOccurs="1">
                        <xsd:element name="effectif-present-ou-transhumant" type="EffectifPresentOuTranshumant" minOccurs="0" maxOccurs="unbounded"/>
                        <xsd:element name="effectif-present-ou-transhumant-corse" type="EffectifPresentOuTranshumant" minOccurs="0" maxOccurs="unbounded"/>
                        <xsd:element name="effectif-present" type="EffectifPresent" minOccurs="0" maxOccurs="unbounded"/>
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="numeros-sire" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="numero-sire" type="SIRE" maxOccurs="10"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="existence-UGB-bovines" type="xsd:boolean" use="optional"/>
    </xsd:complexType>

    <!-- type Effectif present ou transhumant pour: ovins, caprins, Ã©quidÃ©s, lamas, alpagas, cerfs et biches, daims et daines, truies reproductrices, autres porcins -->
    <xsd:complexType name="EffectifPresentOuTranshumant">
        <xsd:all>
            <xsd:element name="type-animal-1" type="TypeAnimauxType1" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="nb-animaux-1" type="NbAnimauxType1" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="effectifs-transhumant" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="effectif-transhumant" type="EffectifTranshumant" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>

    <!-- type Effectif present ou transhumant pour: poules pondeuses et autres volailles et lapins -->
    <xsd:complexType name="EffectifPresent">
        <xsd:all>
            <xsd:element name="type-animal-2" type="TypeAnimauxType2" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="nb-animaux-2" type="NbAnimauxType2" minOccurs="0" maxOccurs="1"/>
        </xsd:all>
    </xsd:complexType>

    <!-- type Effectif transhumant -->
    <xsd:complexType name="EffectifTranshumant">
        <xsd:sequence>
            <xsd:choice minOccurs="1" maxOccurs="1">
                <xsd:element name="nb-animaux-envoyes" type="NbAnimauxType1"/>
                <xsd:element name="nb-animaux-recus" type="NbAnimauxType1"/>
            </xsd:choice>
            <xsd:choice minOccurs="1" maxOccurs="1">
                <xsd:element name="estive" type="xsd:boolean"/>
                <xsd:element name="hivernage" type="xsd:boolean"/>
            </xsd:choice>
            <xsd:element name="nom-exploitant" type="NomExploitant" minOccurs="1"/>
            <xsd:element name="departement" type="CodeDepartement" minOccurs="1"/>
            <xsd:element name="commune" type="CodeInseeCommune" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- type PJ -->
    <xsd:complexType name="PiecesJointes">
        <xsd:sequence>
            <xsd:element name="pj" type="PieceJointe" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="PieceJointe">
        <xsd:all>
            <xsd:element name="intitule" minOccurs="1" maxOccurs="1"/>
        </xsd:all>
        <xsd:attribute name="code-type-pj" use="required"/>
        <xsd:attribute name="type-pj" use="required"/>
    </xsd:complexType>

    <!-- type BlocNote -->
    <xsd:complexType name="BlocNote">
        <xsd:all>
            <xsd:element name="commentaire" type="Commentaire" minOccurs="0" maxOccurs="1"/>
        </xsd:all>
    </xsd:complexType>

    <!-- type AutresObligations -->
    <xsd:complexType name="AutresObligations">
        <xsd:attribute name="periode-bcae6" type="PeriodeBCAE6" use="optional"/>
        <xsd:attribute name="autorisation-transmission-donnees" type="xsd:boolean" use="optional"/>
			<!-- la valeur "autorisation-transmission-donnees" doit rester optionnelle pour la campagne N-1 (donc 2023) -->
    </xsd:complexType>

    <!-- type RPG -->
    <xsd:complexType name="RPG">
        <xsd:sequence>
            <xsd:element name="ilot" type="IlotRpg" minOccurs="1" maxOccurs="unbounded"/>
            <xsd:element name="sna-declaree" type="SnaDeclaree" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="zdh-declaree" type="ZdhDeclaree" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="IlotRpg">
        <xsd:all>
            <xsd:element name="commune" type="CodeInseeCommune" minOccurs="0"/>
            <xsd:element name="geometrie" type="GeometrieType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="parcelles" type="ParcellesIlot" minOccurs="0"/>
            <xsd:element name="elements-bio" type="ElementsBio" minOccurs="0"/>
            <xsd:element name="elements-maec-S" type="ElementsMaecS" minOccurs="0"/>
            <xsd:element name="elements-maec-L" type="ElementsMaecL" minOccurs="0"/>
            <xsd:element name="elements-maec-P" type="ElementsMaecP" minOccurs="0"/>
        </xsd:all>
        <xsd:attribute name="numero-ilot" type="NumeroIlot" use="required"/>
        <xsd:attribute name="numero-ilot-reference" type="NumeroIlotReference" use="optional"/>
    </xsd:complexType>

    <!-- type ParcellesIlot -->
    <xsd:complexType name="ParcellesIlot">
        <xsd:sequence>
            <xsd:element name="parcelle" type="ParcelleIlot" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- type ParcelleIlot -->
    <xsd:complexType name="ParcelleIlot">
        <xsd:sequence>
            <xsd:element name="descriptif-parcelle" type="DescriptifParcelle"/>
            <xsd:element name="geometrie" type="GeometrieType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="surface-admissible" type="SurfaceAres" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="portee" type="xsd:string" minOccurs="0" />
        </xsd:sequence>
    </xsd:complexType>

    <!-- type ElementsBio -->
    <xsd:complexType name="ElementsBio">
        <xsd:sequence>
            <xsd:element name="element-bio" type="ElementBio" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- type ElementsMaecS -->
    <xsd:complexType name="ElementsMaecS">
        <xsd:sequence>
            <xsd:element name="element-surfacique" type="ElementMaecS" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- type ElementsMaecL -->
    <xsd:complexType name="ElementsMaecL">
        <xsd:sequence>
            <xsd:element name="element-lineaire" type="ElementL" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- type ElementsMaecP -->
    <xsd:complexType name="ElementsMaecP">
        <xsd:sequence>
            <xsd:element name="element-ponctuel" type="ElementP" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- type ElementBio -->
    <xsd:complexType name="ElementBio">
        <xsd:all>
            <xsd:element name="numero-element" type="NumeroElement" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="code-mesure" type="MesureMaec" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="cultures-annuelles" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="geometrie" type="GeometrieType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="premiere-campagne" type="Annee" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="derniere-campagne" type="Annee" minOccurs="0" maxOccurs="1"/>
        </xsd:all>
    </xsd:complexType>

    <!-- type ElementMaecS -->
    <xsd:complexType name="ElementMaecS">
        <xsd:all>
            <xsd:element name="numero-element" type="NumeroElement" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="code-mesure" type="MesureMaec" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="semis-sous-couvert" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="geometrie" type="GeometrieType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="premiere-campagne" type="Annee" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="derniere-campagne" type="Annee" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="sous-type-geometrie" type="SousTypeGeometrie" minOccurs="0" maxOccurs="1"/>
        </xsd:all>
    </xsd:complexType>

    <!-- type Element L -->
    <xsd:complexType name="ElementL">
        <xsd:sequence>
            <xsd:element name="numero-element" type="NumeroElement" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="code-mesure" type="MesureMaec" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="geometrie" type="GeometrieTypeL" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="premiere-campagne" type="Annee" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="derniere-campagne" type="Annee" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- type Element P -->
    <xsd:complexType name="ElementP">
        <xsd:sequence>
            <xsd:element name="numero-element" type="NumeroElement" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="code-mesure" type="MesureMaec" minOccurs="1" maxOccurs="1"/>
