{
  "name": "@agencebio/cartobio-api",
  "version": "2.11.0",
  "description": "api for cartobio",
  "main": "app.js",
  "bin": {
    "cartobio-export-asp": "./bin/export-asp.js"
  },
  "dependencies": {
    "@agencebio/rosetta-cultures": "^1.0.5",
    "@fastify/cors": "^8.2.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^7.3.0",
    "@fastify/oauth2": "^7.0.0",
    "@fastify/swagger": "^8.2.1",
    "@fastify/swagger-ui": "^1.3.0",
    "@sentry/integrations": "^7.36.0",
    "@sentry/node": "^7.28.1",
    "@turf/helpers": "^6.5.0",
    "adm-zip": "^0.5.10",
    "ajv-formats": "^2.1.1",
    "convict": "^6.2.3",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.3",
    "fast-jwt": "^2.0.2",
    "fastify": "^4.11.0",
    "form-data": "^4.0.0",
    "get-stream": "^6.0.0",
    "got": "^11.8.6",
    "jsonstream-next": "^3.0.0",
    "mnemonist": "^0.39.5",
    "p-memoize": "^4.0.4",
    "pg": "^8.7.3",
    "pg-query-stream": "^4.2.4",
    "reproject": "^1.2.6",
    "strip-bom-stream": "^4.0.0",
    "through2": "^4.0.2",
    "validator": "^13.7.0",
    "verrazzano": "^2.3.0",
    "wkt": "^0.1.1"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "devDependencies": {
    "@turf/centroid": "^6.5.0",
    "@types/convict": "^6.1.4",
    "@types/geojson": "^7946.0.10",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.10.3",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^29.3.1",
    "minimist": "^1.2.8",
    "nodemon": "^2.0.19",
    "supertest": "^6.3.3",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "load:communes-table": "node bin/build-communes-table.js",
    "build:communes-data": "node bin/build-communes-data.js",
    "build:types": "tsc lib/outputs/*.js --declaration --allowJs --emitDeclarationOnly --outDir docs/types",
    "export-asp": "node bin/export-asp.js --numerobio-path cartobio-asp.csv | ogr2ogr cartobio-asp.gpkg /vsistdin/",
    "export-asp-c1": "node bin/export-asp.js --only-c1 --numerobio-path cartobio-asp-c1.csv | ogr2ogr cartobio-asp-c1.gpkg /vsistdin/",
    "postinstall": "npm run build:communes-data",
    "prestart": "./node_modules/.bin/db-migrate up && npm run load:communes-table",
    "lint": "eslint --ignore-pattern 'migrations/*' .",
    "test": "NODE_ENV=test jest --detectOpenHandles --collectCoverage --coverageReporters html --coverageReporters text-summary --coverageReporters lcovonly",
    "posttest": "npm run lint && npm run build:types",
    "prewatch": "node ./node_modules/.bin/db-migrate up",
    "watch": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/entrepreneur-interet-general/CartoBio-API.git"
  },
  "keywords": [
    "cartobio",
    "nodejs",
    "api"
  ],
  "author": "Bchini Lucas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/entrepreneur-interet-general/CartoBio-API/issues"
  },
  "homepage": "https://github.com/entrepreneur-interet-general/CartoBio-API#readme"
}
